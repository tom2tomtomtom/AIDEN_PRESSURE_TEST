    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "playwright": "^1.57.0",
    "postcss": "^8.4.38",
    "postgres": "^3.4.8",
    "prettier": "^3.3.0",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "supabase": "^1.180.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
